class Solution {
public:
    int arraySign(vector<int>& nums) {
        int check = 1;
        int ans[1001] ={0};
        ans[0] = 1;
        for(int i = 0 ; i < nums.size() ; i++){
            int tmp = nums[i];
            if(tmp < 0){
                tmp = -tmp;
                check = check * -1;
            }
            for(int j = 0 ; j < 1000 ; j++){
                ans[j] = ans[j] * tmp;
            }
            for(int j = 0 ; j < 1000 ; j++){
                while(ans[j] >= 10){
                    ans[j+1] = ans[j+1] + ans[j] / 10;
                    ans[j] = ans[j] % 10;
                }
            }
        }
        int cnt = 1000;
        while(ans[cnt] == 0){
            cnt--;
            if(cnt == -1){
                return 0;
            }
        }
        if(check == 1){
            return 1;
        }
        else if(check == -1){
            return -1;
        }
        return 0;
    }
};
