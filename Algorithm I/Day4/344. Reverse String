class Solution {
public:
    void reverseString(vector<char>& s) {
        int length = s.size();
        vector<char> s1(length);
        for(int i = 0 , j = length - 1; i < length ; i++ , j--){
            s1[j] = s[i];
        }
       s = s1;
    }
};
